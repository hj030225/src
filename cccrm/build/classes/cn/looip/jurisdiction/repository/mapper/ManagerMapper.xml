<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.looip.jurisdiction.repository.dao.JurisdictionDAO" >
  <resultMap id="ManageMap" type="cn.looip.jurisdiction.repository.domain.Manager" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="login_name" property="loginName" jdbcType="VARCHAR" />
    <result column="login_pwd" property="loginPwd" jdbcType="VARCHAR" />
    <result column="user_email" property="userEmail" jdbcType="VARCHAR" />
    <result column="user_mobile" property="userMobile" jdbcType="VARCHAR" />
    <result column="user_message" property="userMessage" jdbcType="INTEGER" />
    <result column="user_type" property="userType" jdbcType="SMALLINT" />
    <result column="user_role_id" property="userRoleId" jdbcType="INTEGER" />
  </resultMap>
  
 
  
  <insert id="insertProgrammer2" parameterType="cn.looip.jurisdiction.repository.domain.Manager" >
  insert into sys_user(login_name,login_pwd,user_mobile,user_message,user_type)
  values(#{loginName},#{loginPwd},#{userMobile},#{userMessage},1);
  </insert>

</mapper>